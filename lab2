      "source": [
        "size=data.shape[0]\n",
        "data_mean=np.mean(data)\n",
        "def CI_printout(data, interval = 0.95, method = 't'):\n",
        "  mean_val = data.mean()\n",
        "  n = data.count()\n",
        "  stdev = data.std()\n",
        "  if method == 't':\n",
        "    test_stat = stats.t.ppf((interval + 1)/2, n)\n",
        "  elif method == 'z':\n",
        "    test_stat = stats.norm.ppf((interval + 1)/2)\n",
        "  lower_bound = mean_val - test_stat * stdev / math.sqrt(n)\n",
        "  upper_bound = mean_val + test_stat * stdev / math.sqrt(n)\n",
        "  return mean_val,lower_bound,upper_bound\n",
        "\n",
        "CI_printout(data, interval = 0.95, method = 't')\n"
      ],
